snippet debug "print a variable" !b
echo '<pre>';
var_dump(${0:${VISUAL}});
echo '</pre>';
endsnippet

snippet xdebug "print a variable and exit 42" !b
echo '<pre>';
var_dump(${0:${VISUAL}});
echo '</pre>';
exit(42);
endsnippet

