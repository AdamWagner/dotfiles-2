{
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "workbench.startupEditor": "none",
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "vscode-icons",
  "vsicons.presets.foldersAllDefaultIcon": true,
  "editor.renderIndentGuides": false,
  "workbench.editor.enablePreview": false,
  "editor.rulers": [80],
  "editor.fontLigatures": false,
  "editor.cursorBlinking": "solid",
  "editor.folding": false,
  "editor.dragAndDrop": false,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.showIcons": true,
  "workbench.activityBar.visible": false,
  "window.title": "${folderName}",
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/coverage/**": false
  },
  "window.zoomLevel": 0,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "Fira Code",
  "typescript.npm": "/usr/local/bin/npm",
  "explorer.confirmDragAndDrop": false,
  "search.useIgnoreFiles": true,
  "extensions.ignoreRecommendations": false,
  "eslint.enable": true,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true
  },
  "[rust]": {
    "editor.formatOnSave": true
  },
  "[terraform]": {
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.formatOnSave": true
  },
  "editor.trimAutoWhitespace": true,
  "files.trimTrailingWhitespace": true,
  "workbench.statusBar.feedback.visible": false,
  "explorer.confirmDelete": false,
  "prettier.eslintIntegration": true,
  "files.hotExit": "off",
  "workbench.tree.horizontalScrolling": true,
  "editor.codeLens": false,
  "vsicons.dontShowNewVersionMessage": true,
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "editor.formatOnPaste": true,
  "breadcrumbs.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotionMarkerFontFamily": "Fira Code",
  "vim.easymotionMarkerForegroundColorOneChar": "black",
  "vim.easymotionMarkerForegroundColorTwoChar": "black",
  "vim.easymotionMarkerBackgroundColor": "orange",
  "vim.easymotionKeys": "fjdkslaghrueiwoqptyvmcxzbn",
  "yaml.format.enable": false,
  "scm.diffDecorations": "none"
}
