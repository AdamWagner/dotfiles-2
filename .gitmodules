#Vim
[submodule "_vim/vim/autoload/pathogen"]
	path = _vim/vim/autoload/pathogen
	url = git://github.com/tpope/vim-pathogen.git
        ignore = dirty
[submodule "_vim/vim/bundle/js/javascript"]
	path = _vim/vim/bundle/js/javascript
	url = git://github.com/pangloss/vim-javascript.git
        ignore = dirty
[submodule "_vim/vim/bundle/js/json"]
	path = _vim/vim/bundle/js/json
	url = git://github.com/jakar/vim-json.git
        ignore = dirty
[submodule "_vim/vim/bundle/multiple_cursors"]
	path = _vim/vim/bundle/multiple_cursors
	url = git://github.com/terryma/vim-multiple-cursors.git
        ignore = dirty
[submodule "_vim/vim/bundle/surround"]
	path = _vim/vim/bundle/surround
	url = git://github.com/tpope/vim-surround.git
        ignore = dirty

#Xcode
[submodule "xcode/plugins/xvim"]
	path = xcode/plugins/xvim
	url = git://github.com/JugglerShu/XVim.git
        ignore = dirty
