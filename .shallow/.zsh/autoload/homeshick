# Lazy loading (once)
${__homeshick_loaded:-false} || {

  fpath=(
    "$HOME/.homesick/repos/homeshick/completions"
    "${fpath[@]}"
  )
  source "$HOME/.homesick/repos/homeshick/homeshick.sh"

__homeshick_loaded=true }

# Run
case "$(type -p "$0")" in
  *not\ found) "$0" "$@" ;;
  *) command "$0" "$@" ;;
esac
