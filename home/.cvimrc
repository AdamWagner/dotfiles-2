set noautofocus
set autoupdategist
set cncpcompletion
set nativelinkorder
set sortlinkhints
set vimcommand

let barposition = "bottom"
let blacklists = ["http://www.typing.com/*"]

"
" Colemak stuff
"

" Colemak homerow
let hintcharacters = "arstdhneio"

" Feels like Apple natural scrolling
" j and k already behave properly on the Colemak layout
map u scrollFullDown
map d scrollFullUp
map <C-u> scrollFullPageDown
map <C-d> scrollFullPageUp
