set noautofocus
set autoupdategist
set cncpcompletion
set nativelinkorder
set sortlinkhints
set vimcommand

let blacklists = ["http://www.typing.com/*"]
" Colemak homerow, sorted considering accessibility on a matrix keyboard for a
" right-handed person
" See: https://i1.wp.com/www.workmanlayout.com/blog/wp-content/uploads/2010/10/keyboard_graded_grid.png
let hintcharacters = "ntesiroahd"
let searchalias g = "google"

map <C-d> scrollFullPageDown
map <C-u> scrollFullPageUp
